{
    "name": "klipper/resource-bundle",
    "type": "symfony-bundle",
    "description": "Resource bundle of Klipper",
    "keywords": ["resource", "doctrine", "batch", "manager", "validation"],
    "homepage": "https://github.com/klipperdev/resource-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0",
        "symfony/framework-bundle": "^5.1.0",
        "klipper/resource": "^2.0.0"
    },
    "require-dev": {
        "klipper/default-value-bundle": "^2.0.0",
        "symfony/form": "^5.1.0"
    },
    "autoload": {
        "psr-4": { "Klipper\\Bundle\\ResourceBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
